{
  "name": "google-tasks",
  "displayName": "Google Tasks",
  "description": "Access Google Tasks list in the Tree View of VSCode",
  "publisher": "KrishnaPravin",
  "repository": {
    "type": "git",
    "url": "https://github.com/KrishnaPravin/google-tasks-vscode-extension"
  },
  "bugs": {
    "url": "https://github.com/KrishnaPravin/google-tasks-vscode-extension/issues"
  },
  "icon": "resources/external/marketPlaceIcon.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:googleTasks"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "google-tasks",
          "title": "Google Tasks",
          "icon": "resources/extIcon.png"
        }
      ]
    },
    "views": {
      "google-tasks": [
        {
          "id": "googleTasks",
          "name": "Tasks List"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.14",
    "googleapis": "^45.0.0",
    "googleapis-common": "^3.1.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "vscode": "^1.1.36",
    "vscode-test": "^1.2.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
